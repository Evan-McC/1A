#Function prints your name
#Function prints out your name

def my_function():
  print("My name is Evan McCroddan")

my_function()

print("")

def my_function(fname):
  print(fname + " McCroddan")

my_function("Evan")
my_function("Mark")
my_function("Stephen")

print("")

def my_function(fname, lname):
  print(fname + " " + lname)

my_function("Evan", "McCroddan")
